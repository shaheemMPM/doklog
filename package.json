{
	"name": "AWSLogs",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node src/index.ts",
		"build": "tsc -p tsconfig.json",
		"start": "node dist/index.js",
		"package": "pnpm build && pkg dist/index.js -t node18-macos-x64,node18-linux-x64,node18-win-x64 -o bin/cloudwatch-lens",
		"check": "biome check .",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"fix": "biome check --write ."
	},
	"bin": "dist/index.js",
	"pkg": {
		"assets": [
			"node_modules/**/*"
		],
		"outputPath": "bin"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.13.1",
	"dependencies": {
		"@aws-sdk/client-cloudwatch-logs": "^3.932.0",
		"@inquirer/password": "^4.0.23",
		"@inquirer/prompts": "^7.10.1",
		"boxen": "^8.0.1",
		"chalk": "^5.6.2",
		"dotenv": "^17.2.3",
		"ini": "^6.0.0",
		"minimist": "^1.2.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"@types/ini": "^4.1.1",
		"@types/minimist": "^1.2.5",
		"@types/node": "^24.10.1",
		"@yao-pkg/pkg": "^6.10.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	}
}
