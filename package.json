{
	"name": "doklog",
	"version": "1.0.0",
	"description": "AWS logs, simplified - Part of the Dokops toolkit",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node src/index.ts",
		"build": "tsc -p tsconfig.json",
		"start": "node dist/index.js",
		"package": "pnpm build && pkg dist/index.js -t node18-macos-x64,node18-linux-x64,node18-win-x64 -o bin/doklog",
		"package:macos": "pnpm build && pkg dist/index.js -t node18-macos-x64 -o bin/doklog-macos",
		"package:linux": "pnpm build && pkg dist/index.js -t node18-linux-x64 -o bin/doklog-linux",
		"package:win": "pnpm build && pkg dist/index.js -t node18-win-x64 -o bin/doklog-win.exe",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"test:run": "vitest run",
		"release": "./scripts/release.sh",
		"check": "biome check .",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"fix": "biome check --write .",
		"prepare": "husky"
	},
	"bin": "dist/index.js",
	"pkg": {
		"assets": [
			"node_modules/**/*"
		],
		"outputPath": "bin"
	},
	"keywords": [
		"aws",
		"cloudwatch",
		"lambda",
		"logs",
		"cli",
		"monitoring",
		"dokops",
		"aws-cli",
		"terminal",
		"cloudwatch-logs",
		"aws-lambda",
		"devops",
		"observability"
	],
	"author": "Mohammed Shaheem P <shaheemmpm@gmail.com> (https://github.com/shaheemMPM)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/shaheemMPM/doklog.git"
	},
	"bugs": {
		"url": "https://github.com/shaheemMPM/doklog/issues"
	},
	"homepage": "https://github.com/shaheemMPM/doklog#readme",
	"engines": {
		"node": ">=18.0.0"
	},
	"packageManager": "pnpm@10.13.1",
	"dependencies": {
		"@aws-sdk/client-cloudwatch-logs": "^3.932.0",
		"@aws-sdk/client-lambda": "^3.932.0",
		"@inquirer/password": "^4.0.23",
		"@inquirer/prompts": "^7.10.1",
		"boxen": "5.1.2",
		"chalk": "4.1.2",
		"dotenv": "^17.2.3",
		"ini": "^6.0.0",
		"minimist": "^1.2.8"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.5",
		"@types/ini": "^4.1.1",
		"@types/minimist": "^1.2.5",
		"@types/node": "^24.10.1",
		"@vitest/coverage-v8": "4.0.9",
		"@vitest/ui": "^4.0.9",
		"@yao-pkg/pkg": "^6.10.1",
		"husky": "^9.1.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3",
		"vitest": "^4.0.9"
	}
}
